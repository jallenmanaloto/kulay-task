{
  "expo": {
    "name": "kulay",
    "slug": "kulay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/placeholder.png",
    "scheme": "kulay",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/placeholder.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/placeholder.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.alleeeeenm.kulay"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/placeholder.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "81612bcb-ed75-40c8-b587-1bc39252725b"
      }
    }
  }
}
